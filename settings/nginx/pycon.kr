upstream app_server {
        server 127.0.0.1:9000;
}

server {
        listen 80;

        root /usr/share/nginx/html;

        # Make site accessible from http://localhost/
        server_name dev.pycon.kr;

        location / {
                uwsgi_pass app_server;
                include uwsgi_params;
        }
        location /static {
                alias /home/deploy/pyconkr/collected_static/;
        }
}
